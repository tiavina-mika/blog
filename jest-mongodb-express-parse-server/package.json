  "scripts": {
    "start": "cross-env NODE_ENV=production node --enable-source-maps dist/index.js",
    "start:dev": "cross-env NODE_ENV=development node --enable-source-maps dist/index.js",
    "start:preprod": "cross-env NODE_ENV=preProduction node --enable-source-maps dist/index.js",
    "dev": "npx concurrently \"node config/esbuild/build.watch.config.js\" \"npx nodemon\"",
    "lint:check": "eslint",
    "lint": "eslint . --fix",
    "copy": "node config/copyBuilds",
    "build:bo:preprod": "cd ../bo && npm run build:preprod && cd ../server && npm run copy",
    "build:bo": "cd ../bo && npm run build:prod && cd ../server && cross-env NODE_ENV=production npm run copy",
    "build": "node config/esbuild/build.config.js",
    "depcheck": "npx depcheck",
    "prepare": "husky",
    "lint-staged": "lint-staged",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "jest watch",
    "test:coverage": "jest --coverage"
  },
